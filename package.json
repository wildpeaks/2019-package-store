    "@types/sinon": "9.0.9",
    "@wildpeaks/eslint-config-typescript": "10.7.0",
    "@wildpeaks/tsconfig-web": "2.11.1",
    "@wildpeaks/webpack-config-web": "5.16.0",
    "eslint": "7.16.0",
    "express": "4.17.1",
    "mocha": "8.2.1",
    "playwright": "1.7.1",
    "preact": "10.5.7",
    "prettier": "2.2.1",
    "recursive-readdir": "2.2.2",
    "rimraf": "3.0.2",
    "sinon": "9.2.2",
    "ts-loader": "8.0.12",
    "ts-node": "9.1.1",
    "typescript": "4.1.3",
    "webpack": "4.44.2"
  }
}
